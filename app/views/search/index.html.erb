<h1>Search#index</h1>
<p>Find me in app/views/search/index.html.erb</p>

<%= form_tag("/search/index", method: "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>

<% @search.each do |x| %>
	<h1><%= x["title"] %></h1>
	<ul>
	<li><%= x["locations"] %></li>
</ul>
<% end %>